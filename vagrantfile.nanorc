# Here is an example for yml files.

syntax "Vagrantfile" "Vagrantfile"

#Base directives
color green "[a-zA-Z0-9]+\.vm\.[a-zA-Z0-9]+|end|Vagrant.configure|config.vm.define|\|.+\||do"

#Parameters
color brightgreen ":\w+"
color brightyellow ".+\s+=\s+.+"

# Comments
color blue "(^|[[:space:]]+)#.*$"
color cyan "^[[:space:]]*##.*$"
